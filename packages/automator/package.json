{
  "name": "@insta-puppeteer/automator",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'tsx' src/main.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "start": "node dist/main.js",
    "format": "biome format",
    "lint": "biome lint",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "yarn@4.7.0",
  "dependencies": {
    "@insta-puppeteer/database": "workspace:*",
    "dotenv": "^17.2.1",
    "puppeteer": "^24.16.1"
  },
  "devDependencies": {
    "@insta-puppeteer/typescript-config": "workspace:*",
    "@types/node": "^20",
    "nodemon": "^3.1.9",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5"
  }
}
